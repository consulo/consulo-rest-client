<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <psi.referenceContributor language="http"
                                  implementation="org.javamaster.httpclient.reference.HttpUrlPsiReferenceContributorCoolRequest"/>
        <psi.referenceContributor language="JSON"
                                  implementation="org.javamaster.httpclient.reference.JsonKeyPsiReferenceContributorCoolRequest"/>

        <lang.documentationProvider language="http"
                                    implementationClass="org.javamaster.httpclient.doc.HttpUrlDocumentationProviderCoolRequest"/>
        <lang.documentationProvider language="JSON"
                                    implementationClass="org.javamaster.httpclient.doc.JsonKeyDocumentationProviderCoolRequest"/>
    </extensions>
</idea-plugin>