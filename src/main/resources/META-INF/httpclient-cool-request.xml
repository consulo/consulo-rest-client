<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <psi.referenceContributor language="http"
                                  implementation="org.javamaster.httpclient.reference.HttpUrlControllerMethodPsiReferenceContributorCoolRequest"/>
        <psi.referenceContributor language="JSON"
                                  implementation="org.javamaster.httpclient.reference.JsonKeyControllerMethodFieldPsiReferenceContributorCoolRequest"/>

        <lang.documentationProvider language="http"
                                    implementationClass="org.javamaster.httpclient.doc.HttpUrlControllerMethodDocumentationProviderCoolRequest"/>
        <psi.treeChangePreprocessor
                implementation="org.javamaster.httpclient.doc.support.ControllerPsiTreeChangePreprocessor"/>
    </extensions>
</idea-plugin>